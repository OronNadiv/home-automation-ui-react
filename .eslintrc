{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel",
    "react",
    "promise",
    "standard"
  ],
  "globals": {
    "__DEV__": true,
    "__TEST__": true,
    "__PROD__": true,
    "__COVERAGE__": true,
    "NODE_ENV": true,
    "process": true,
    "__BASENAME__": true
  }
}
