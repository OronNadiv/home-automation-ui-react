    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.5.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-photoswipe": "^1.2.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redbox-react": "^1.3.6",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "request-promise": "^4.1.1",
    "sane-email-validation": "^1.1.0",
