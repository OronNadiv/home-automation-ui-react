{
  "name": "home-automation-ui-react",
  "private": true,
  "author": "Oron Nadiv <oron@nadiv.us> (https://github.com/OronNadiv/)",
  "homepage": "https://github.com/OronNadiv/home-automation-ui-react/",
  "bugs": "https://github.com/OronNadiv/home-automation-ui-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/OronNadiv/home-automation-ui-react.git"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": "^7",
    "npm": "^3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "start": "cross-env NODE_ENV=development node build/scripts/start",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "postinstall": "ln -sfn ../ node_modules/ha"
  },
  "betterScripts": {
    "compile": {
      "command": "npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "compression": "^1.6.2",
    "debug": "^3.1.0",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
