    "immutable": "^3.8.2",
    "material-ui": "^0.20.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "photoswipe": "^4.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
